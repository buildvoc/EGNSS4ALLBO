{% extends "base.html.twig" %}
{% block title %}Change Password{% endblock %}
{% block body %}

<h2></h2>              
<div class="w-100">
  <div class="js_change_password_inputs box_dropdown mt-0 mb-3">
    <h2 id="js_fi_header">Change Password</h2>
    <form id="change_password_form" method="POST" action="index.php">
    <input type="hidden" name="act" id="js_fi_act" value="change_password">
    <input type="hidden" name="user_id" id="js_fi_uid" value="{{ user_id }}">
    <table class="form_table mt-3">
      <thead>
        <!--tr>
          <td class="pass_input" ><label>Current Password</label><input id="js_fi_oldpass" type="password" name="old_password" required><span class="show_password"><i class="fas fa-eye"></i></span></td>
        </tr-->
        <tr>
          <td class="pass_input"><label>New Password</label><input id="js_fi_newpass" type="password" name="new_password" required="" style="cursor: auto;"><span class="show_password"><i class="fas fa-eye"></i></span></td>
        </tr>
        <tr>
          <td class="pass_input"><label>New Password Confirm</label><input id="js_fi_newpassconf" type="password" name="new_password_confirm" required="" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;"><span class="show_password"><i class="fas fa-eye"></i></span></td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <button class="js_change_password btn btn-primary mt-3" type="button">CONFIRM</button>
          </td>
          <td></td>
        </tr>
      </tbody>
    </table>
    </form>
  </div>
</div>
            
            
